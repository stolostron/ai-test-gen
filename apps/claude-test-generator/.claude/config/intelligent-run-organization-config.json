{
  "intelligent_run_organization": {
    "enabled": true,
    "description": "Intelligent folder organization for multiple runs of same JIRA ticket",
    "version": "1.0.0",
    
    "organization_rules": {
      "auto_migrate_threshold": 2,
      "preserve_single_runs": true,
      "latest_metadata_enabled": true,
      "backup_before_migration": false,
      "organization_validation": true
    },
    
    "folder_structure": {
      "single_run_format": "{ticket}-{timestamp}",
      "ticket_parent_format": "{ticket}",
      "run_within_ticket_format": "{ticket}-{timestamp}",
      "latest_metadata_file": "latest-run-metadata.json"
    },
    
    "migration_settings": {
      "automatic_migration": true,
      "preserve_existing_structure": true,
      "rollback_capability": true,
      "cleanup_incomplete_runs": true
    },
    
    "patterns": {
      "ticket_regex": "^[A-Z]+-\\d+$",
      "timestamp_format": "%Y%m%d-%H%M%S",
      "individual_run_pattern": "^{ticket}-\\d{8}-\\d{6}$",
      "incomplete_run_pattern": "^{ticket}-(?!.*\\d{8}-\\d{6}$).*$"
    },
    
    "integration_points": {
      "phase_0_pre": "environment_selection_with_path_determination",
      "phase_4": "test_generation_with_organized_output",
      "phase_5": "cleanup_and_organization_validation",
      "metadata_service": "enhanced_metadata_generation",
      "observability": "real_time_organization_monitoring"
    },
    
    "validation_requirements": {
      "data_preservation": "zero_data_loss_guarantee",
      "structure_integrity": "all_files_and_metadata_maintained",
      "reference_consistency": "internal_links_updated",
      "framework_integration": "services_locate_runs_properly"
    },
    
    "performance_optimization": {
      "lazy_migration": "only_reorganize_on_second_run",
      "minimal_file_operations": "efficient_moves_vs_copies",
      "metadata_caching": "cache_organization_analysis",
      "batch_operations": "group_filesystem_operations"
    },
    
    "error_handling": {
      "migration_failures": "rollback_to_original_structure",
      "directory_conflicts": "handle_name_conflicts_gracefully",
      "permission_issues": "graceful_filesystem_error_handling",
      "partial_migrations": "recover_from_interrupted_operations"
    }
  }
}