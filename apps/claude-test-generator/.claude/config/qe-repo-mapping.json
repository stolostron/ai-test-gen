{
  "qe_repository_mapping": {
    "description": "Predefined mapping of JIRA components to QE automation repositories - Complete feature coverage prioritized over duplication avoidance",
    "mapping_rules": {
      "cluster_lifecycle": {
        "jira_components": ["Cluster Lifecycle", "cluster-lifecycle", "cluster lifecycle"],
        "primary_repo": "stolostron/clc-ui-e2e",
        "additional_repos": [
          "stolostron/acmqe-clc-test"
        ],
        "excluded_repos": [
          "stolostron/cluster-lifecycle-e2e"
        ],
        "usage_notes": {
          "primary_repo": "stolostron/clc-ui-e2e - Main UI-based cluster lifecycle testing repository",
          "acmqe-clc-test": "Non-UI/API repository - Use only when specifically mentioned, not for general analysis",
          "cluster-lifecycle-e2e": "EXCLUDED - Not team-managed, do not analyze or reference"
        }
      },
      "application_lifecycle": {
        "jira_components": ["Application Lifecycle", "app-lifecycle", "application lifecycle"],
        "primary_repo": "stolostron/application-ui-test",
        "additional_repos": []
      },
      "governance": {
        "jira_components": ["Governance", "governance", "policy", "GRC"],
        "primary_repo": "stolostron/governance-policy-framework",
        "additional_repos": [
          "stolostron/governance-policy-propagator",
          "stolostron/acmqe-grc-test"
        ]
      },
      "search": {
        "jira_components": ["Search", "search", "search-api"],
        "primary_repo": "stolostron/acmqe-search-test",
        "additional_repos": [
          "open-cluster-management/search-e2e-test",
          "stolostron/acmqe-search-api"
        ]
      },
      "observability": {
        "jira_components": ["Observability", "observability", "monitoring"],
        "primary_repo": "stolostron/multicluster-observability-operator",
        "additional_repos": []
      },
      "console": {
        "jira_components": ["Console", "console", "UI"],
        "primary_repo": "stolostron/console",
        "additional_repos": [
          "stolostron/clc-ui-e2e"
        ]
      },
      "cicd": {
        "jira_components": ["CI/CD", "cicd", "automation", "pipeline"],
        "primary_repo": "stolostron/acmqe-autotest",
        "additional_repos": []
      }
    },
    "fallback_strategy": {
      "enabled": true,
      "search_method": "gh_search_repos",
      "search_patterns": [
        "stolostron {component} test",
        "stolostron {component} e2e",
        "stolostron {component}"
      ],
      "excluded_from_fallback": [
        "stolostron/cluster-lifecycle-e2e"
      ],
      "log_fallback": true
    },
    "coverage_policy": {
      "priority": "complete_feature_coverage",
      "duplication_tolerance": "minor_overlap_acceptable",
      "principle": "Better to have minor duplication than miss critical test scenarios"
    }
  }
}