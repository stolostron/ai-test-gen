{
  "implementation_reality_agent": {
    "execution_phase": "0.5",
    "priority": "HIGHEST",
    "blocking_authority": true,
    "mandatory_before": [
      "documentation_intelligence",
      "github_investigation", 
      "test_generation",
      "advanced_reasoning"
    ],
    "user_codebase_paths": [
      "/Users/ashafi/Documents/work/automation/clc-ui",
      "/Users/ashafi/Documents/work/ai/ai_systems/apps/claude-test-generator/temp_repos/clc-ui-e2e"
    ],
    "evidence_requirements": {
      "ui_workflow_validation": {
        "required_files": ["*.js", "*.jsx", "*.vue", "*.html"],
        "selector_patterns": ["cy.get", "cy.contains", "find", "click"],
        "navigation_patterns": ["visit", "navigate", "go to", "url"]
      },
      "schema_validation": {
        "required_files": ["*.yaml", "*.yml", "*.json"],
        "schema_sources": ["crd", "template", "config", "fixture"],
        "field_validation": "strict"
      },
      "cli_pattern_validation": {
        "required_files": ["*.js", "*.spec.js", "*.test.js"],
        "command_patterns": ["cy.exec", "oc", "kubectl", "updateJobTemplateYamlByCli"],
        "validation_methods": ["actual_usage", "help_documentation"]
      }
    },
    "blocking_conditions": {
      "fictional_ui_elements": {
        "block_if": "ui_test_generation && !ui_evidence_found",
        "error_message": "Cannot generate UI tests without evidence of UI functionality"
      },
      "invalid_schema_fields": {
        "block_if": "yaml_field_usage && !field_in_schema",
        "error_message": "Cannot use YAML fields not found in actual schemas"
      },
      "pattern_invention": {
        "block_if": "new_test_pattern && !existing_pattern_match",
        "error_message": "Must extend existing patterns, not invent new ones"
      }
    }
  },
  "integration_settings": {
    "framework_halt_on_evidence_failure": true,
    "require_evidence_for_all_claims": true,
    "prioritize_user_codebase": true,
    "validate_against_actual_implementation": true
  }
}