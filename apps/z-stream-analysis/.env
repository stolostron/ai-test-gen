# Z-Stream Analysis Environment Configuration

# Jenkins Authentication
JENKINS_TOKEN=your_jenkins_api_token_here
JENKINS_USERNAME=your_jenkins_username_here

# Cluster Authentication (choose one method per cluster)
# Token-based authentication
CLUSTER_TOKEN=your_cluster_token_here

# Username/Password authentication
CLUSTER_USERNAME=your_cluster_username_here
CLUSTER_PASSWORD=your_cluster_password_here

# Certificate-based authentication
CLUSTER_CERT_PATH=/path/to/cluster.crt
CLUSTER_KEY_PATH=/path/to/cluster.key

# Optional: Default settings
DEFAULT_CLUSTER_ENV=qe6
DEFAULT_BRANCH=main
DEFAULT_ANALYSIS_TYPE=complete

# Notification Settings
CLAUDE_NOTIFICATIONS_ENABLED=true
CLAUDE_NOTIFICATION_SOUND=true
CLAUDE_NOTIFICATION_VOLUME=0.7
CLAUDE_NOTIFICATION_EVENTS="ai_response_complete,analysis_complete,error_occurred"
CLAUDE_NOTIFICATION_PERSIST=true
